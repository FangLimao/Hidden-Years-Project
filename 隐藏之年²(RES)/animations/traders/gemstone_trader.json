{
  "format_version": "1.8.0",
  "animations": {
    "animation.gemstone_trader.baby_transform": {
      "loop": true,
      "bones": {
        "head": {
          "scale": 1.5
        }
      }
    },
    "animation.gemstone_trader.general": {
      "loop": true,
      "bones": {
        "arms": {
          "position": [0, -1, -1],
          "rotation": ["-42.97-this", 0, 0]
        }
      }
    },
    "animation.gemstone_trader.general.v1.0": {
      "loop": true,
      "bones": {
        "arms": {
          "position": [0, "-3-this", "-1-this"],
          "rotation": ["-42.97-this", 0, 0]
        }
      }
    },
    "animation.gemstone_trader.get_in_bed": {
      "loop": true,
      "bones": {
        "body": {
          "position": [0, 1, -15],
          "rotation": [-90, 0, 0]
        }
      }
    },
    "animation.gemstone_trader.move": {
      "anim_time_update": "query.modified_distance_moved",
      "loop": true,
      "bones": {
        "leg0": {
          "rotation": ["(math.cos(query.anim_time*38.17)*40)-this", "-this", 0]
        },
        "leg1": {
          "rotation": [
            "(math.cos(query.anim_time*38.17+180)*40)-this",
            "-this",
            0
          ]
        }
      }
    },
    "animation.gemstone_trader.raise_arms": {
      "loop": true,
      "bones": {
        "arms": {
          "rotation": ["variable.raise_arms*-15", 0, 0]
        }
      }
    }
  }
}
