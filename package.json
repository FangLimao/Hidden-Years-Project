{
  "name": "hy2",
  "version": "1.0.0",
  "description": "A Minecraft addon",
  "private": true,
  "scripts": {
    "build": "esbuild src/index.ts --bundle --format=esm --external:@minecraft/server --external:@minecraft/server-ui --minify-whitespace --minify-syntax \"--outfile=./隐藏之年2(BP)/scripts/index.js\"",
