  "dependencies": {
    "@minecraft/server": "1.11.0-beta.1.20.80-stable",
    "@minecraft/server-ui": "1.2.0-beta.1.20.80-stable",
    "@minecraft/vanilla-data": "1.20.80",
    "typescript": "5.5.0-beta",
    "esbuild": "^0.21.3",
    "sapi-utils": "^0.2.0",
    "sapi-ui-utils": "^0.3.0"
  }
}
